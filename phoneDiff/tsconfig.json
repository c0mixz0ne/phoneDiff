{
  "compilerOptions": {
    "module": "ESNext",
		"moduleResolution": "node",
		"target": "ESNext",
		"sourceMap": true,
		"strict": true,
		"lib": [
			"DOM",
			"ESNext"
		],
		"esModuleInterop": true,
		"noImplicitAny": true,
		"noEmitOnError": true,
  },
  "include": [
    "src",
    "env.d.ts"
  ],
  "exclude": [
    ".vscode",
    "node_modules",
    "public"
  ],
  "compileOnSave": false
}
